{
    "name" : "los/losdomain",
    "description" : "ZF2/ZF3 module for use configuration, layout, database, etc per domain or subdomain",
    "require" : {
        "php" : "^5.6 || ^7.0",
        "zendframework/zend-modulemanager": "^2.8",
        "zendframework/zend-servicemanager": "^3.3",
        "zendframework/zend-mvc": "^3.1",
        "zendframework/zend-eventmanager": "^3.2"
    },
    "require-dev": {
        "satooshi/php-coveralls": "dev-master",
        "phpunit/phpunit": "^6.3"
    },
    "suggest": {
        "los/losbase": "los/losbase for some abstract classes to speed development ",
        "los/loslog": "los/loslog for logging",
        "los/losui": "los/losui for twitter bootstrap 3 styling, jquery, chosen and other libraries"
    },
    "authors" : [
        {
            "name" : "Leandro Silva",
            "email" : "leandro@leandrosilva.info",
            "homepage" : "http://leandrosilva.info"
        }
    ],
    "keywords" : [ "zf3", "domain", "module", "zf2" ],
    "autoload" : {
        "psr-4" : {
            "LosDomain\\" : "src/"
        }
    },
    "type" : "library",
    "license" : "MIT",
    "homepage" : "http://github.com/Lansoweb/LosDomain"
}
